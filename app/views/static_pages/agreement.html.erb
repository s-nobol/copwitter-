<script src="https://code.jquery.com/jquery-3.0.0.js"></script>
<script>
    $(function(){
      var main_offset= $("#main_top").offset().top;
      var bar = $("#bar");
      var sub_title=[]
      var title = []
      var font_color= "deepskyblue" 

      // タイトルとサブタイトルを格納
      $('#bar p').each(function(i, e) {
          sub_title[i] = $(e)
      })
      $('h1.text-blue').each(function(i, e) {
          title[i] = $(e)
      })
      
        //ページ内スクロール
      $('#bar p').click(function () {
          let i = $('#bar p').index(this)
          var p = title[i].offset().top;
          $('body,html').animate({ scrollTop : p }, 500, 'swing'); 
          return false;
      });
      
      // スクロールイベント
      $(window).scroll(function() {
        var scroll_size = $(window).scrollTop() + 80
          if(scroll_size > main_offset){
            bar.css("position","fixed") 
            bar.css("top","40px")
            
            for ( var i = 0; i<sub_title.length; i++ ) {
                sub_title[i].css("color","black")
              
              // var next_top =sub_title.length + i + 1 - sub_title.length;
                  
                  // マックスの時以外回す
              if(i+1 === sub_title.length){
                    if(scroll_size >= title[sub_title.length-1].offset().top){
                    sub_title[i].css("color",font_color)
                  }
                }else{
                  if(scroll_size >= title[i].offset().top && scroll_size < title[i+1].offset().top){
                    sub_title[i].css("color",font_color)
                  }
                }  
            }
            
          }else{
            bar.css("position","static") 
            bar.css("color","black")
          }
      });
      
      
    });
</script>